<div class="row">
    <div class="col-md-2">
        <h1>All Posts</h1>
    </div>
    <div class="col-md-4">
        <%= link_to "Add Post", new_post_path(@post), class: 'btn btn-success' %>
    </div>
</div>

<% @posts.each do |post| %>
    <div class="media">
        <div class="media-body">
            <h4 class="media-heading">
                <%= link_to post.title, post %>
            </h4>
        </div>
    </div>
<% end %>

