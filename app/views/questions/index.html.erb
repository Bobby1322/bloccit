<div class="row">
    <div class="col-md-2">
        <h1>All Questions</h1>
    </div>
    <div class="col-md-4">
        <%= link_to "Add Question", new_question_path(@question), class: 'btn btn-success' %>
    </div>
</div>

<% @questions.each do |f| %>
    <div class="media">
        <div class="media-body">
            <h4 class="media-heading"></h4>
                <%= link_to f.title, f %>
            </h4>
        </div>
    </div>
<% end %>

